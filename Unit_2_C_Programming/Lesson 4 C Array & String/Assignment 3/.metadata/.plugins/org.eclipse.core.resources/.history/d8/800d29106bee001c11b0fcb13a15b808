# include "stdio.h"

int main ()
{
	    char s[100];
		char c;
        int i,freq=0;
		printf("Enter a string: ");
		fflush(stdout); fflush(stdin);
		scanf("%s",s);

		printf("Enter a characterto find frequency: ");
				fflush(stdout); fflush(stdin);
				scanf("%c",&c);
        for(i=0;i!=0;i++)
        {
        	if(c== 's[i]')
        		freq++;
        }


}
